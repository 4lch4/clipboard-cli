{
	"name": "clipboard-cli",
	"version": "2.1.0",
	"description": "Access the system clipboard (copy/paste)",
	"license": "MIT",
	"repository": "4lch4/clipboard-cli",
	"author": {
		"name": "Sindre Sorhus",
		"email": "sindresorhus@gmail.com",
		"url": "sindresorhus.com"
	},
	"bin": {
		"clipboard": "cli.js"
	},
	"engines": {
		"node": ">=8"
	},
	"scripts": {
		"test": "xo && ava",
		"pack": "pkg --out-path=dist/ . && np --no-publish"
	},
	"files": [
		"cli.js"
	],
	"pkg": {
		"targets": [
			"node8-linux",
			"node8-win",
			"node8-macos",
			"node10-linux",
			"node10-win",
			"node10-macos",
			"node12-linux",
			"node12-win",
			"node12-macos",
			"node14-linux",
			"node14-win",
			"node14-macos"
		]
	},
	"keywords": [
		"cli-app",
		"cli",
		"clipboard",
		"copy",
		"paste",
		"copy-paste",
		"pasteboard",
		"read",
		"write",
		"pbcopy",
		"clip",
		"xclip",
		"xsel"
	],
	"dependencies": {
		"clipboardy": "^2.0.0",
		"get-stdin": "^7.0.0",
		"meow": "^5.0.0"
	},
	"devDependencies": {
		"ava": "^1.4.1",
		"execa": "^1.0.0",
		"xo": "^0.24.0",
		"pkg": "^4.4.9",
		"np": "^7.4.0"
	}
}
